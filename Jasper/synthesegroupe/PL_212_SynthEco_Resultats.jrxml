	<queryString>
		<![CDATA[SELECT DISTINCT t1.libelletypeculture AS culture,
unnest(array[
'Marges brutes (€/ha)',
'Charges globales (€/ha)',
'Prix de vente (€/t)',
'Rendement (U récolte/ha)'
]) AS clef,
unnest(array[
'sum(ve.marge)/sum(ve.surface)',
'sum(ve.charges)/sum(ve.surface)',
'CASE WHEN t1.idtypeculture>=2000
THEN SUM(ve.produits-ve.produits_autres)/NULLIF(SUM(ve.qte_recolte),0)
ELSE 10*SUM(ve.produits-ve.produits_autres)/NULLIF(SUM(ve.qte_recolte),0)
END',
'sum(ve.qte_recolte)/sum(ve.surface)'
]) AS valeur

FROM bdanalysegroupe.type_culture t1

WHERE
-- critere sur les cultures selectionnees
$X{IN, CAST(t1.idtypeculture AS varchar), p_cultures}

ORDER BY culture, clef]]>
	</queryString>
