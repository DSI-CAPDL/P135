	<queryString>
		<![CDATA[SELECT 	t0.*,
	CASE 	WHEN SUBSTRING(clef FROM LENGTH(clef)-11 FOR 12) LIKE 'sous contrat' THEN 'MAE 2'
		WHEN SUBSTRING(clef FROM LENGTH(clef)-11 FOR 12) LIKE 'hors contrat' THEN 'sans'
	END AS sous_contrat
FROM(
SELECT DISTINCT t1.libelletypeculture AS culture,
unnest(array[
'IFT Herbicides sur parcelles sous contrat',
'IFT Hors Herbicides sur parcelles sous contrat',
'IFT Herbicides sur parcelles hors contrat',
'IFT Hors Herbicides sur parcelles hors contrat'
]) AS clef,
unnest(array[
'COALESCE(ROUND((SUM(vp.ift_herb*vp.surface)/SUM(vp.surface))::numeric,2),0)',
'COALESCE(ROUND((SUM(vp.ift_horsherb*vp.surface)/SUM(vp.surface))::numeric,2),0)'
]) AS valeur
FROM bdanalysegroupe.type_culture t1

WHERE
-- critere sur les cultures selectionnees
$X{IN, CAST(t1.idtypeculture AS varchar), p_cultures}
) t0
ORDER BY culture, clef]]>
	</queryString>
